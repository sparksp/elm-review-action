{
  "name": "elm-review-action",
  "version": "0.1.0",
  "description": "Use elm-review to check code quality",
  "main": "lib/main.js",
  "scripts": {
    "postinstall": "elm-tooling install",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "run-s build format lint pack test ;"
  },
  "homepage": "https://github.com/sparksp/elm-review-action#readme",
  "bugs": {
    "url": "https://github.com/sparksp/elm-review-action/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sparksp/elm-review-action.git"
  },
  "keywords": [
    "actions",
    "elm-review",
    "lint"
  ],
  "author": "Phill Sparks <me@phills.me.uk> (https://github.com/sparksp)",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "@octokit/action": "^4.0.8"
  },
  "devDependencies": {
    "@octokit/types": "^7.5.1",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.3",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vercel/ncc": "^0.34.0",
    "elm-review": "^2.7.5",
    "elm-tooling": "^1.8.0",
    "eslint": "^8.0",
    "eslint-plugin-github": "^5.0.0-2",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-prettier": "^5.0",
    "jest": "^29.1.2",
    "jest-circus": "^29.1.2",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}